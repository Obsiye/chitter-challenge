<head>
  <title>Chitter</title>
  <link rel="stylesheet" href="<%= url('./stylesheets/index.css')%>">
</head>
<body>

  <h1>Chitter</h1>

<div class='page'>
  <form action='/peeps/new' method='post'>
		<input type='text' id='peep' name='peep' placeholder="What's happening?">
		<input type='submit' name='submit' value='submit'>
  </form>

    <% @peeps.each do |peep| %>
      <ul>
        <li id='description'><%= peep.description %>  </li>
        <li id='time'><%= peep.creation_time %></li>
      </ul>
    <% end %>
</div>
</body>
